            {% include "components/donate_qr.html" %}
        </div>
    </main>
    {% include "components/footer.html" %}
    <script>
        const noElement = document.querySelector('#home-ownership-no');
        const yesElement = document.querySelector('#home-ownership-yes');
        const infoElement = document.querySelector('#landlord-info');
        let display;
        noElement.addEventListener('click', (e) => {
        display = infoElement.classList.contains('hidden');
            if (display) {
                infoElement.classList.remove('hidden')
                infoElement.classList.add('block')
            }
        })

        yesElement.addEventListener('click', (e) => {
        display = infoElement.classList.contains('block');
            if (display) {
                infoElement.classList.remove('block')
                infoElement.classList.add('hidden')
            }
        })
    </script>
</body>

